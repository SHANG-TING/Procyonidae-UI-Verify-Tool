    },
    "procyonidae-electron": {
      "root": "apps/procyonidae-electron",
      "sourceRoot": "apps/procyonidae-electron/src",
      "projectType": "application",
      "prefix": "procyonidae-electron",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/procyonidae-electron",
            "main": "apps/procyonidae-electron/src/main.ts",
            "tsConfig": "apps/procyonidae-electron/tsconfig.app.json",
            "assets": ["apps/procyonidae-electron/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/procyonidae-electron/src/environments/environment.ts",
                  "with": "apps/procyonidae-electron/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "procyonidae-electron:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "procyonidae-electron",
            "frontendProject": "procyonidae",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "procyonidae-electron",
            "frontendProject": "procyonidae",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/procyonidae-electron/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/procyonidae-electron"],
          "options": {
            "jestConfig": "apps/procyonidae-electron/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
