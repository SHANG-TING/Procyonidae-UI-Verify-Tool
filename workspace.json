    "procyonidae": {
      "root": "apps/procyonidae",
      "sourceRoot": "apps/procyonidae/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/procyonidae",
            "index": "apps/procyonidae/src/index.html",
            "main": "apps/procyonidae/src/main.tsx",
            "polyfills": "apps/procyonidae/src/polyfills.ts",
            "tsConfig": "apps/procyonidae/tsconfig.app.json",
            "assets": [
              "apps/procyonidae/src/favicon.ico",
              "apps/procyonidae/src/assets"
            ],
            "styles": ["apps/procyonidae/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/procyonidae/src/environments/environment.ts",
                  "with": "apps/procyonidae/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "procyonidae:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "procyonidae:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/procyonidae/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/procyonidae"],
          "options": {
            "jestConfig": "apps/procyonidae/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "procyonidae-e2e": {
      "root": "apps/procyonidae-e2e",
      "sourceRoot": "apps/procyonidae-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/procyonidae-e2e/cypress.json",
            "tsConfig": "apps/procyonidae-e2e/tsconfig.e2e.json",
            "devServerTarget": "procyonidae:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "procyonidae:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/procyonidae-e2e/**/*.{js,ts}"]
          }
        }
